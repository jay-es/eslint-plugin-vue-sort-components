{
  "name": "@jay-es/eslint-plugin-vue-sort-components",
  "version": "0.1.0",
  "description": "A plugin for ESLint to keep order of component names",
  "main": "lib/index.js",
  "files": [
    "README.md",
    "lib"
  ],
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --check ./**/*.js",
    "lint": "eslint .",
    "test": "mocha tests/**/*.js",
    "test:watch": "mocha --watch --reporter min tests/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jay-es/eslint-plugin-vue-sort-components.git"
  },
  "keywords": [
    "eslint",
    "vue",
    "sort",
    "components",
    "auto-fix",
    "order"
  ],
  "author": "jay-es",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jay-es/eslint-plugin-vue-sort-components/issues"
  },
  "homepage": "https://github.com/jay-es/eslint-plugin-vue-sort-components#readme",
  "lint-staged": {
    "*.js": [
      "prettier --check",
      "eslint",
      "mocha"
    ]
  },
  "dependencies": {
    "natural-compare": "^1.4.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.5",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1"
  }
}
